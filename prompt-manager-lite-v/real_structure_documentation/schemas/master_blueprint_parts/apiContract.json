{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://schemas.promptmanager.dev/api-contract/v3.0.0",
    "$comment": "Playbook: prompt_playbooks/schemas_playbooks/playbook_schema-apiContract.md",
    "title": "Universal API Contract Schema Template",
    "description": "Universal template for documenting API contracts that works across any API type, protocol, or technology stack. This schema provides a structured approach to API contract documentation using placeholders that can be adapted to REST, GraphQL, gRPC, WebSocket, or any other API paradigm. Based on OpenAPI 3.1 specification and modern API design patterns for 2025.",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contractInfo",
        "apiSpecification",
        "endpoints",
        "dataModels",
        "authentication"
    ],
    "properties": {
        "contractInfo": {
            "type": "object",
            "description": "Basic API contract information and metadata",
            "required": [
                "apiName",
                "version",
                "description",
                "apiType"
            ],
            "additionalProperties": false,
            "properties": {
                "apiName": {
                    "type": "string",
                    "description": "API name - use placeholder [API_NAME]",
                    "examples": [
                        "[API_NAME]",
                        "[PROJECT_NAME] API",
                        "[SERVICE_NAME] Interface"
                    ]
                },
                "version": {
                    "type": "string",
                    "description": "API version - use placeholder [API_VERSION]",
                    "pattern": "^\\[.*\\]$|^\\d+\\.\\d+\\.\\d+$",
                    "examples": [
                        "[API_VERSION]",
                        "[MAJOR].[MINOR].[PATCH]"
                    ]
                },
                "description": {
                    "type": "string",
                    "description": "API description - use placeholder [API_DESCRIPTION]",
                    "examples": [
                        "[API_DESCRIPTION]",
                        "API for [API_PURPOSE]"
                    ]
                },
                "apiType": {
                    "type": "string",
                    "description": "API type/protocol - use placeholder [API_TYPE]",
                    "examples": [
                        "[API_TYPE]",
                        "REST",
                        "GraphQL",
                        "gRPC",
                        "WebSocket",
                        "Event-Driven",
                        "[CUSTOM_API_TYPE]"
                    ]
                },
                "baseUrl": {
                    "type": "string",
                    "description": "Base URL - use placeholder [BASE_URL]",
                    "examples": [
                        "[BASE_URL]",
                        "[PROTOCOL]://[DOMAIN]/[API_PATH]"
                    ]
                },
                "contact": {
                    "type": "object",
                    "description": "Contact information using placeholders",
                    "properties": {
                        "name": {
                            "type": "string",
                            "examples": [
                                "[CONTACT_NAME]",
                                "[API_OWNER]"
                            ]
                        },
                        "email": {
                            "type": "string",
                            "examples": [
                                "[CONTACT_EMAIL]",
                                "[API_SUPPORT_EMAIL]"
                            ]
                        },
                        "url": {
                            "type": "string",
                            "examples": [
                                "[CONTACT_URL]",
                                "[DOCUMENTATION_URL]"
                            ]
                        }
                    }
                },
                "license": {
                    "type": "object",
                    "description": "License information using placeholders",
                    "properties": {
                        "name": {
                            "type": "string",
                            "examples": [
                                "[LICENSE_NAME]",
                                "MIT",
                                "Apache 2.0",
                                "[CUSTOM_LICENSE]"
                            ]
                        },
                        "url": {
                            "type": "string",
                            "examples": [
                                "[LICENSE_URL]",
                                "[LICENSE_DOCUMENT_URL]"
                            ]
                        }
                    }
                }
            }
        },
        "apiSpecification": {
            "type": "object",
            "description": "API specification details using placeholders",
            "required": [
                "specificationFormat",
                "contentType"
            ],
            "properties": {
                "specificationFormat": {
                    "type": "string",
                    "description": "Specification format - use placeholder [SPEC_FORMAT]",
                    "examples": [
                        "[SPEC_FORMAT]",
                        "OpenAPI 3.1",
                        "GraphQL Schema",
                        "Protocol Buffers",
                        "AsyncAPI",
                        "[CUSTOM_SPEC_FORMAT]"
                    ]
                },
                "contentType": {
                    "type": "array",
                    "description": "Supported content types using placeholders",
                    "items": {
                        "type": "string",
                        "examples": [
                            "[CONTENT_TYPE]",
                            "application/json",
                            "application/xml",
                            "text/plain",
                            "[CUSTOM_CONTENT_TYPE]"
                        ]
                    }
                },
                "protocols": {
                    "type": "array",
                    "description": "Supported protocols using placeholders",
                    "items": {
                        "type": "string",
                        "examples": [
                            "[PROTOCOL]",
                            "HTTP",
                            "HTTPS",
                            "WebSocket",
                            "gRPC",
                            "[CUSTOM_PROTOCOL]"
                        ]
                    }
                },
                "encoding": {
                    "type": "string",
                    "description": "Default encoding - use placeholder [ENCODING]",
                    "examples": [
                        "[ENCODING]",
                        "UTF-8",
                        "[CUSTOM_ENCODING]"
                    ]
                }
            }
        }
    }
}        
"endpoints": {
    "type": "array",
    "description": "API endpoints definition using placeholders",
    "items": {
        "type": "object",
        "required": [
            "endpointId",
            "path",
            "method",
            "summary"
        ],
        "properties": {
            "endpointId": {
                "type": "string",
                "description": "Unique endpoint identifier - use placeholder [ENDPOINT_ID]",
                "examples": [
                    "[ENDPOINT_ID]",
                    "[OPERATION_NAME]",
                    "[RESOURCE_ACTION]"
                ]
            },
            "path": {
                "type": "string",
                "description": "Endpoint path - use placeholder [ENDPOINT_PATH]",
                "examples": [
                    "[ENDPOINT_PATH]",
                    "/[RESOURCE_NAME]",
                    "/[RESOURCE_NAME]/{[RESOURCE_ID]}"
                ]
            },
            "method": {
                "type": "string",
                "description": "HTTP method or operation type - use placeholder [HTTP_METHOD]",
                "examples": [
                    "[HTTP_METHOD]",
                    "GET",
                    "POST",
                    "PUT",
                    "DELETE",
                    "PATCH",
                    "[CUSTOM_METHOD]"
                ]
            },
            "summary": {
                "type": "string",
                "description": "Endpoint summary - use placeholder [ENDPOINT_SUMMARY]",
                "examples": [
                    "[ENDPOINT_SUMMARY]",
                    "[OPERATION_DESCRIPTION]"
                ]
            },
            "description": {
                "type": "string",
                "description": "Detailed endpoint description - use placeholder [ENDPOINT_DESCRIPTION]",
                "examples": [
                    "[ENDPOINT_DESCRIPTION]",
                    "[DETAILED_OPERATION_DESCRIPTION]"
                ]
            },
            "tags": {
                "type": "array",
                "description": "Endpoint tags for grouping using placeholders",
                "items": {
                    "type": "string",
                    "examples": [
                        "[ENDPOINT_TAG]",
                        "[RESOURCE_CATEGORY]"
                    ]
                }
            },
            "parameters": {
                "type": "array",
                "description": "Endpoint parameters using placeholders",
                "items": {
                    "type": "object",
                    "required": [
                        "name",
                        "in",
                        "type"
                    ],
                    "properties": {
                        "name": {
                            "type": "string",
                            "examples": [
                                "[PARAMETER_NAME]",
                                "[FIELD_NAME]"
                            ]
                        },
                        "in": {
                            "type": "string",
                            "examples": [
                                "[PARAMETER_LOCATION]",
                                "query",
                                "path",
                                "header",
                                "body",
                                "[CUSTOM_LOCATION]"
                            ]
                        },
                        "type": {
                            "type": "string",
                            "examples": [
                                "[PARAMETER_TYPE]",
                                "string",
                                "integer",
                                "boolean",
                                "array",
                                "object",
                                "[CUSTOM_TYPE]"
                            ]
                        },
                        "required": {
                            "type": "boolean",
                            "description": "Whether parameter is required"
                        },
                        "description": {
                            "type": "string",
                            "examples": [
                                "[PARAMETER_DESCRIPTION]",
                                "[FIELD_PURPOSE]"
                            ]
                        },
                        "format": {
                            "type": "string",
                            "examples": [
                                "[PARAMETER_FORMAT]",
                                "date-time",
                                "email",
                                "uuid",
                                "[CUSTOM_FORMAT]"
                            ]
                        },
                        "example": {
                            "description": "Example value using placeholder",
                            "examples": [
                                "[PARAMETER_EXAMPLE]",
                                "[SAMPLE_VALUE]"
                            ]
                        }
                    }
                }
            },
            "requestBody": {
                "type": "object",
                "description": "Request body specification using placeholders",
                "properties": {
                    "description": {
                        "type": "string",
                        "examples": [
                            "[REQUEST_BODY_DESCRIPTION]",
                            "[PAYLOAD_DESCRIPTION]"
                        ]
                    },
                    "required": {
                        "type": "boolean",
                        "description": "Whether request body is required"
                    },
                    "contentType": {
                        "type": "string",
                        "examples": [
                            "[REQUEST_CONTENT_TYPE]",
                            "application/json",
                            "application/xml",
                            "[CUSTOM_CONTENT_TYPE]"
                        ]
                    },
                    "schema": {
                        "type": "string",
                        "description": "Reference to data model - use placeholder [REQUEST_SCHEMA_REF]",
                        "examples": [
                            "[REQUEST_SCHEMA_REF]",
                            "#/dataModels/[MODEL_NAME]"
                        ]
                    },
                    "example": {
                        "description": "Request body example using placeholder",
                        "examples": [
                            "[REQUEST_BODY_EXAMPLE]",
                            "[SAMPLE_PAYLOAD]"
                        ]
                    }
                }
            },
            "responses": {
                "type": "array",
                "description": "Response specifications using placeholders",
                "items": {
                    "type": "object",
                    "required": [
                        "statusCode",
                        "description"
                    ],
                    "properties": {
                        "statusCode": {
                            "type": "string",
                            "description": "HTTP status code - use placeholder [STATUS_CODE]",
                            "examples": [
                                "[STATUS_CODE]",
                                "200",
                                "201",
                                "400",
                                "401",
                                "404",
                                "500",
                                "[CUSTOM_STATUS]"
                            ]
                        },
                        "description": {
                            "type": "string",
                            "examples": [
                                "[RESPONSE_DESCRIPTION]",
                                "[STATUS_DESCRIPTION]"
                            ]
                        },
                        "contentType": {
                            "type": "string",
                            "examples": [
                                "[RESPONSE_CONTENT_TYPE]",
                                "application/json",
                                "application/xml",
                                "[CUSTOM_CONTENT_TYPE]"
                            ]
                        },
                        "schema": {
                            "type": "string",
                            "description": "Reference to data model - use placeholder [RESPONSE_SCHEMA_REF]",
                            "examples": [
                                "[RESPONSE_SCHEMA_REF]",
                                "#/dataModels/[MODEL_NAME]"
                            ]
                        },
                        "headers": {
                            "type": "array",
                            "description": "Response headers using placeholders",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "examples": [
                                            "[HEADER_NAME]",
                                            "[RESPONSE_HEADER]"
                                        ]
                                    },
                                    "type": {
                                        "type": "string",
                                        "examples": [
                                            "[HEADER_TYPE]",
                                            "string",
                                            "integer",
                                            "[CUSTOM_TYPE]"
                                        ]
                                    },
                                    "description": {
                                        "type": "string",
                                        "examples": [
                                            "[HEADER_DESCRIPTION]",
                                            "[HEADER_PURPOSE]"
                                        ]
                                    }
                                }
                            }
                        },
                        "example": {
                            "description": "Response example using placeholder",
                            "examples": [
                                "[RESPONSE_EXAMPLE]",
                                "[SAMPLE_RESPONSE]"
                            ]
                        }
                    }
                }
            },
            "security": {
                "type": "array",
                "description": "Endpoint-specific security requirements using placeholders",
                "items": {
                    "type": "string",
                    "examples": [
                        "[SECURITY_SCHEME_NAME]",
                        "[AUTH_REQUIREMENT]"
                    ]
                }
            },
            "deprecated": {
                "type": "boolean",
                "description": "Whether endpoint is deprecated"
            },
            "externalDocs": {
                "type": "object",
                "description": "External documentation using placeholders",
                "properties": {
                    "description": {
                        "type": "string",
                        "examples": [
                            "[EXTERNAL_DOC_DESCRIPTION]",
                            "[REFERENCE_DESCRIPTION]"
                        ]
                    },
                    "url": {
                        "type": "string",
                        "examples": [
                            "[EXTERNAL_DOC_URL]",
                            "[REFERENCE_URL]"
                        ]
                    }
                }
            }
        }
    }
}   
     "dataModels": {
    "type": "array",
    "description": "Data models and schemas using placeholders",
    "items": {
        "type": "object",
        "required": [
            "modelName",
            "type",
            "properties"
        ],
        "properties": {
            "modelName": {
                "type": "string",
                "description": "Model name - use placeholder [MODEL_NAME]",
                "examples": [
                    "[MODEL_NAME]",
                    "[ENTITY_NAME]",
                    "[DATA_TYPE_NAME]"
                ]
            },
            "type": {
                "type": "string",
                "description": "Model type - use placeholder [MODEL_TYPE]",
                "examples": [
                    "[MODEL_TYPE]",
                    "object",
                    "array",
                    "string",
                    "integer",
                    "boolean",
                    "[CUSTOM_TYPE]"
                ]
            },
            "description": {
                "type": "string",
                "description": "Model description - use placeholder [MODEL_DESCRIPTION]",
                "examples": [
                    "[MODEL_DESCRIPTION]",
                    "[ENTITY_PURPOSE]"
                ]
            },
            "properties": {
                "type": "array",
                "description": "Model properties using placeholders",
                "items": {
                    "type": "object",
                    "required": [
                        "propertyName",
                        "type"
                    ],
                    "properties": {
                        "propertyName": {
                            "type": "string",
                            "examples": [
                                "[PROPERTY_NAME]",
                                "[FIELD_NAME]"
                            ]
                        },
                        "type": {
                            "type": "string",
                            "examples": [
                                "[PROPERTY_TYPE]",
                                "string",
                                "integer",
                                "boolean",
                                "array",
                                "object",
                                "[CUSTOM_TYPE]"
                            ]
                        },
                        "format": {
                            "type": "string",
                            "examples": [
                                "[PROPERTY_FORMAT]",
                                "date-time",
                                "email",
                                "uuid",
                                "[CUSTOM_FORMAT]"
                            ]
                        },
                        "description": {
                            "type": "string",
                            "examples": [
                                "[PROPERTY_DESCRIPTION]",
                                "[FIELD_PURPOSE]"
                            ]
                        },
                        "required": {
                            "type": "boolean",
                            "description": "Whether property is required"
                        },
                        "nullable": {
                            "type": "boolean",
                            "description": "Whether property can be null"
                        },
                        "readOnly": {
                            "type": "boolean",
                            "description": "Whether property is read-only"
                        },
                        "writeOnly": {
                            "type": "boolean",
                            "description": "Whether property is write-only"
                        },
                        "example": {
                            "description": "Property example using placeholder",
                            "examples": [
                                "[PROPERTY_EXAMPLE]",
                                "[SAMPLE_VALUE]"
                            ]
                        },
                        "enum": {
                            "type": "array",
                            "description": "Enumerated values using placeholders",
                            "items": {
                                "examples": [
                                    "[ENUM_VALUE]",
                                    "[ALLOWED_VALUE]"
                                ]
                            }
                        },
                        "minimum": {
                            "type": "number",
                            "description": "Minimum value for numeric types"
                        },
                        "maximum": {
                            "type": "number",
                            "description": "Maximum value for numeric types"
                        },
                        "minLength": {
                            "type": "integer",
                            "description": "Minimum length for string types"
                        },
                        "maxLength": {
                            "type": "integer",
                            "description": "Maximum length for string types"
                        },
                        "pattern": {
                            "type": "string",
                            "description": "Regex pattern for string validation - use placeholder [VALIDATION_PATTERN]",
                            "examples": [
                                "[VALIDATION_PATTERN]",
                                "[REGEX_PATTERN]"
                            ]
                        }
                    }
                }
            },
            "required": {
                "type": "array",
                "description": "Required properties using placeholders",
                "items": {
                    "type": "string",
                    "examples": [
                        "[REQUIRED_PROPERTY]",
                        "[MANDATORY_FIELD]"
                    ]
                }
            },
            "additionalProperties": {
                "type": "boolean",
                "description": "Whether additional properties are allowed"
            },
            "example": {
                "description": "Model example using placeholder",
                "examples": [
                    "[MODEL_EXAMPLE]",
                    "[SAMPLE_OBJECT]"
                ]
            }
        }
    }
},
"authentication": {
    "type": "object",
    "description": "Authentication and security schemes using placeholders",
    "required": [
        "securitySchemes"
    ],
    "properties": {
        "securitySchemes": {
            "type": "array",
            "description": "Available security schemes using placeholders",
            "items": {
                "type": "object",
                "required": [
                    "schemeName",
                    "type"
                ],
                "properties": {
                    "schemeName": {
                        "type": "string",
                        "description": "Security scheme name - use placeholder [SECURITY_SCHEME_NAME]",
                        "examples": [
                            "[SECURITY_SCHEME_NAME]",
                            "[AUTH_METHOD_NAME]"
                        ]
                    },
                    "type": {
                        "type": "string",
                        "description": "Security scheme type - use placeholder [SECURITY_TYPE]",
                        "examples": [
                            "[SECURITY_TYPE]",
                            "apiKey",
                            "http",
                            "oauth2",
                            "openIdConnect",
                            "[CUSTOM_SECURITY_TYPE]"
                        ]
                    },
                    "description": {
                        "type": "string",
                        "examples": [
                            "[SECURITY_DESCRIPTION]",
                            "[AUTH_METHOD_DESCRIPTION]"
                        ]
                    },
                    "scheme": {
                        "type": "string",
                        "description": "HTTP authentication scheme - use placeholder [HTTP_SCHEME]",
                        "examples": [
                            "[HTTP_SCHEME]",
                            "bearer",
                            "basic",
                            "[CUSTOM_SCHEME]"
                        ]
                    },
                    "bearerFormat": {
                        "type": "string",
                        "examples": [
                            "[BEARER_FORMAT]",
                            "JWT",
                            "[TOKEN_FORMAT]"
                        ]
                    },
                    "in": {
                        "type": "string",
                        "description": "API key location - use placeholder [API_KEY_LOCATION]",
                        "examples": [
                            "[API_KEY_LOCATION]",
                            "header",
                            "query",
                            "cookie",
                            "[CUSTOM_LOCATION]"
                        ]
                    },
                    "name": {
                        "type": "string",
                        "description": "API key parameter name - use placeholder [API_KEY_NAME]",
                        "examples": [
                            "[API_KEY_NAME]",
                            "[HEADER_NAME]",
                            "[QUERY_PARAM_NAME]"
                        ]
                    },
                    "flows": {
                        "type": "object",
                        "description": "OAuth2 flows using placeholders",
                        "properties": {
                            "authorizationCode": {
                                "type": "object",
                                "properties": {
                                    "authorizationUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[AUTHORIZATION_URL]",
                                            "[OAUTH_AUTH_ENDPOINT]"
                                        ]
                                    },
                                    "tokenUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[TOKEN_URL]",
                                            "[OAUTH_TOKEN_ENDPOINT]"
                                        ]
                                    },
                                    "refreshUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[REFRESH_URL]",
                                            "[OAUTH_REFRESH_ENDPOINT]"
                                        ]
                                    },
                                    "scopes": {
                                        "type": "object",
                                        "description": "OAuth2 scopes using placeholders",
                                        "patternProperties": {
                                            "^\\[.*\\]$": {
                                                "type": "string",
                                                "examples": [
                                                    "[SCOPE_DESCRIPTION]",
                                                    "[PERMISSION_DESCRIPTION]"
                                                ]
                                            }
                                        }
                                    }
                                }
                            },
                            "implicit": {
                                "type": "object",
                                "properties": {
                                    "authorizationUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[AUTHORIZATION_URL]",
                                            "[OAUTH_AUTH_ENDPOINT]"
                                        ]
                                    },
                                    "refreshUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[REFRESH_URL]",
                                            "[OAUTH_REFRESH_ENDPOINT]"
                                        ]
                                    },
                                    "scopes": {
                                        "type": "object",
                                        "description": "OAuth2 scopes using placeholders",
                                        "patternProperties": {
                                            "^\\[.*\\]$": {
                                                "type": "string",
                                                "examples": [
                                                    "[SCOPE_DESCRIPTION]",
                                                    "[PERMISSION_DESCRIPTION]"
                                                ]
                                            }
                                        }
                                    }
                                }
                            },
                            "password": {
                                "type": "object",
                                "properties": {
                                    "tokenUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[TOKEN_URL]",
                                            "[OAUTH_TOKEN_ENDPOINT]"
                                        ]
                                    },
                                    "refreshUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[REFRESH_URL]",
                                            "[OAUTH_REFRESH_ENDPOINT]"
                                        ]
                                    },
                                    "scopes": {
                                        "type": "object",
                                        "description": "OAuth2 scopes using placeholders",
                                        "patternProperties": {
                                            "^\\[.*\\]$": {
                                                "type": "string",
                                                "examples": [
                                                    "[SCOPE_DESCRIPTION]",
                                                    "[PERMISSION_DESCRIPTION]"
                                                ]
                                            }
                                        }
                                    }
                                }
                            },
                            "clientCredentials": {
                                "type": "object",
                                "properties": {
                                    "tokenUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[TOKEN_URL]",
                                            "[OAUTH_TOKEN_ENDPOINT]"
                                        ]
                                    },
                                    "refreshUrl": {
                                        "type": "string",
                                        "examples": [
                                            "[REFRESH_URL]",
                                            "[OAUTH_REFRESH_ENDPOINT]"
                                        ]
                                    },
                                    "scopes": {
                                        "type": "object",
                                        "description": "OAuth2 scopes using placeholders",
                                        "patternProperties": {
                                            "^\\[.*\\]$": {
                                                "type": "string",
                                                "examples": [
                                                    "[SCOPE_DESCRIPTION]",
                                                    "[PERMISSION_DESCRIPTION]"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "openIdConnectUrl": {
                        "type": "string",
                        "examples": [
                            "[OPENID_CONNECT_URL]",
                            "[OIDC_DISCOVERY_ENDPOINT]"
                        ]
                    }
                }
            }
        },
        "globalSecurity": {
            "type": "array",
            "description": "Global security requirements using placeholders",
            "items": {
                "type": "object",
                "patternProperties": {
                    "^\\[.*\\]$": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "examples": [
                                "[REQUIRED_SCOPE]",
                                "[PERMISSION_NAME]"
                            ]
                        }
                    }
                }
            }
        }
    }
},
"errorHandling": {
    "type": "object",
    "description": "Error handling patterns using placeholders",
    "properties": {
        "errorFormat": {
            "type": "string",
            "description": "Standard error response format - use placeholder [ERROR_FORMAT]",
            "examples": [
                "[ERROR_FORMAT]",
                "RFC 7807 Problem Details",
                "Custom Error Format",
                "[STANDARD_ERROR_FORMAT]"
            ]
        },
        "errorCodes": {
            "type": "array",
            "description": "Standard error codes using placeholders",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string",
                        "examples": [
                            "[ERROR_CODE]",
                            "[APPLICATION_ERROR_CODE]"
                        ]
                    },
                    "message": {
                        "type": "string",
                        "examples": [
                            "[ERROR_MESSAGE]",
                            "[ERROR_DESCRIPTION]"
                        ]
                    },
                    "httpStatus": {
                        "type": "integer",
                        "description": "Associated HTTP status code"
                    }
                }
            }
        },
        "errorSchema": {
            "type": "string",
            "description": "Reference to error response model - use placeholder [ERROR_SCHEMA_REF]",
            "examples": [
                "[ERROR_SCHEMA_REF]",
                "#/dataModels/ErrorResponse"
            ]
        }
    }
},
