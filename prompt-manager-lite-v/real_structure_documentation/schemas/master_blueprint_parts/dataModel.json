{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://schemas.promptmanager.dev/data-model/v3.0.0",
    "$comment": "Playbook: prompt_playbooks/schemas_playbooks/playbook_schema-dataModel.md",
    "title": "Universal Data Model Schema Template",
    "description": "Universal template for documenting data models that works across any database technology, data storage system, or architectural pattern. This schema provides a structured approach to data modeling using placeholders that can be adapted to relational databases, NoSQL systems, data warehouses, or any other data storage paradigm. Follows modern data modeling patterns and best practices for 2025.",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "modelInfo",
        "entities",
        "relationships",
        "constraints"
    ],
    "properties": {
        "modelInfo": {
            "type": "object",
            "description": "Basic data model information and metadata",
            "required": [
                "modelName",
                "version",
                "description",
                "dataParadigm"
            ],
            "additionalProperties": false,
            "properties": {
                "modelName": {
                    "type": "string",
                    "description": "Data model name - use placeholder [MODEL_NAME]",
                    "examples": [
                        "[MODEL_NAME]",
                        "[DOMAIN] Data Model",
                        "[SYSTEM_NAME] Schema"
                    ]
                },
                "version": {
                    "type": "string",
                    "description": "Model version - use placeholder [MODEL_VERSION]",
                    "examples": [
                        "[MODEL_VERSION]",
                        "[MAJOR].[MINOR].[PATCH]"
                    ]
                },
                "description": {
                    "type": "string",
                    "description": "Model description - use placeholder [MODEL_DESCRIPTION]",
                    "examples": [
                        "[MODEL_DESCRIPTION]",
                        "Data model for [DOMAIN_PURPOSE]"
                    ]
                },
                "dataParadigm": {
                    "type": "string",
                    "description": "Data paradigm - use placeholder [DATA_PARADIGM]",
                    "examples": [
                        "[DATA_PARADIGM]",
                        "Relational",
                        "Document",
                        "Graph",
                        "Key-Value",
                        "Column-Family",
                        "Time-Series",
                        "Multi-Model"
                    ]
                },
                "databaseType": {
                    "type": "string",
                    "description": "Database type - use placeholder [DATABASE_TYPE]",
                    "examples": [
                        "[DATABASE_TYPE]",
                        "[DATABASE_TECHNOLOGY]",
                        "[STORAGE_SYSTEM]"
                    ]
                },
                "domain": {
                    "type": "string",
                    "description": "Business domain - use placeholder [BUSINESS_DOMAIN]",
                    "examples": [
                        "[BUSINESS_DOMAIN]",
                        "E-commerce",
                        "Healthcare",
                        "Finance",
                        "[INDUSTRY_DOMAIN]"
                    ]
                },
                "modelingApproach": {
                    "type": "string",
                    "description": "Modeling approach - use placeholder [MODELING_APPROACH]",
                    "examples": [
                        "[MODELING_APPROACH]",
                        "Entity-Relationship",
                        "Dimensional",
                        "Document-Oriented",
                        "Graph-Based",
                        "[CUSTOM_APPROACH]"
                    ]
                },
                "compliance": {
                    "type": "array",
                    "description": "Compliance requirements using placeholders",
                    "items": {
                        "type": "string",
                        "examples": [
                            "[COMPLIANCE_STANDARD]",
                            "GDPR",
                            "HIPAA",
                            "SOX",
                            "PCI-DSS",
                            "[REGULATION]"
                        ]
                    }
                }
            }
        },
        "entities": {
            "type": "array",
            "description": "Data entities/collections/tables using placeholders",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": [
                    "name",
                    "type",
                    "description",
                    "attributes"
                ],
                "additionalProperties": false,
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Entity name - use placeholder [ENTITY_NAME]",
                        "examples": [
                            "[ENTITY_NAME]",
                            "[TABLE_NAME]",
                            "[COLLECTION_NAME]",
                            "[NODE_TYPE]"
                        ]
                    },
                    "type": {
                        "type": "string",
                        "description": "Entity type - use placeholder [ENTITY_TYPE]",
                        "examples": [
                            "[ENTITY_TYPE]",
                            "Table",
                            "Collection",
                            "Document",
                            "Node",
                            "Edge",
                            "View",
                            "[STORAGE_TYPE]"
                        ]
                    },
                    "description": {
                        "type": "string",
                        "description": "Entity description - use placeholder [ENTITY_DESCRIPTION]",
                        "examples": [
                            "[ENTITY_DESCRIPTION]",
                            "[ENTITY_PURPOSE]"
                        ]
                    },
                    "category": {
                        "type": "string",
                        "description": "Entity category - use placeholder [ENTITY_CATEGORY]",
                        "examples": [
                            "[ENTITY_CATEGORY]",
                            "Master Data",
                            "Transactional",
                            "Reference",
                            "Audit",
                            "[DATA_CLASSIFICATION]"
                        ]
                    },
                    "attributes": {
                        "type": "array",
                        "description": "Entity attributes/fields/properties using placeholders",
                        "minItems": 1,
                        "items": {
                            "type": "object",
                            "required": [
                                "name",
                                "dataType",
                                "description"
                            ],
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "description": "Attribute name - use placeholder [ATTRIBUTE_NAME]",
                                    "examples": [
                                        "[ATTRIBUTE_NAME]",
                                        "[FIELD_NAME]",
                                        "[PROPERTY_NAME]",
                                        "[COLUMN_NAME]"
                                    ]
                                },
                                "dataType": {
                                    "type": "string",
                                    "description": "Data type - use placeholder [DATA_TYPE]",
                                    "examples": [
                                        "[DATA_TYPE]",
                                        "String",
                                        "Integer",
                                        "Boolean",
                                        "Date",
                                        "Decimal",
                                        "JSON",
                                        "Array",
                                        "[CUSTOM_TYPE]"
                                    ]
                                },
                                "description": {
                                    "type": "string",
                                    "description": "Attribute description - use placeholder [ATTRIBUTE_DESCRIPTION]",
                                    "examples": [
                                        "[ATTRIBUTE_DESCRIPTION]",
                                        "[FIELD_PURPOSE]"
                                    ]
                                },
                                "constraints": {
                                    "type": "object",
                                    "description": "Attribute constraints using placeholders",
                                    "properties": {
                                        "required": {
                                            "type": "boolean",
                                            "description": "Is required - use placeholder [IS_REQUIRED]",
                                            "examples": [
                                                "[IS_REQUIRED]"
                                            ]
                                        },
                                        "unique": {
                                            "type": "boolean",
                                            "description": "Is unique - use placeholder [IS_UNIQUE]",
                                            "examples": [
                                                "[IS_UNIQUE]"
                                            ]
                                        },
                                        "primaryKey": {
                                            "type": "boolean",
                                            "description": "Is primary key - use placeholder [IS_PRIMARY_KEY]",
                                            "examples": [
                                                "[IS_PRIMARY_KEY]"
                                            ]
                                        },
                                        "foreignKey": {
                                            "type": "object",
                                            "description": "Foreign key reference using placeholders",
                                            "properties": {
                                                "referencedEntity": {
                                                    "type": "string",
                                                    "examples": [
                                                        "[REFERENCED_ENTITY]",
                                                        "[PARENT_TABLE]"
                                                    ]
                                                },
                                                "referencedAttribute": {
                                                    "type": "string",
                                                    "examples": [
                                                        "[REFERENCED_ATTRIBUTE]",
                                                        "[PARENT_COLUMN]"
                                                    ]
                                                }
                                            }
                                        },
                                        "length": {
                                            "type": "integer",
                                            "description": "Maximum length - use placeholder [MAX_LENGTH]",
                                            "examples": [
                                                "[MAX_LENGTH]"
                                            ]
                                        },
                                        "minValue": {
                                            "type": "number",
                                            "description": "Minimum value - use placeholder [MIN_VALUE]",
                                            "examples": [
                                                "[MIN_VALUE]"
                                            ]
                                        },
                                        "maxValue": {
                                            "type": "number",
                                            "description": "Maximum value - use placeholder [MAX_VALUE]",
                                            "examples": [
                                                "[MAX_VALUE]"
                                            ]
                                        },
                                        "pattern": {
                                            "type": "string",
                                            "description": "Validation pattern - use placeholder [VALIDATION_PATTERN]",
                                            "examples": [
                                                "[VALIDATION_PATTERN]",
                                                "[REGEX_PATTERN]"
                                            ]
                                        },
                                        "allowedValues": {
                                            "type": "array",
                                            "description": "Allowed values using placeholders",
                                            "items": {
                                                "examples": [
                                                    "[ALLOWED_VALUE]",
                                                    "[ENUM_OPTION]"
                                                ]
                                            }
                                        },
                                        "defaultValue": {
                                            "description": "Default value - use placeholder [DEFAULT_VALUE]",
                                            "examples": [
                                                "[DEFAULT_VALUE]",
                                                "[INITIAL_VALUE]"
                                            ]
                                        }
                                    }
                                },
                                "businessRules": {
                                    "type": "array",
                                    "description": "Business rules using placeholders",
                                    "items": {
                                        "type": "string",
                                        "examples": [
                                            "[BUSINESS_RULE]",
                                            "[VALIDATION_RULE]",
                                            "[BUSINESS_CONSTRAINT]"
                                        ]
                                    }
                                },
                                "dataClassification": {
                                    "type": "string",
                                    "description": "Data classification - use placeholder [DATA_CLASSIFICATION]",
                                    "examples": [
                                        "[DATA_CLASSIFICATION]",
                                        "Public",
                                        "Internal",
                                        "Confidential",
                                        "Restricted",
                                        "PII",
                                        "Sensitive"
                                    ]
                                },
                                "dataSource": {
                                    "type": "string",
                                    "description": "Data source - use placeholder [DATA_SOURCE]",
                                    "examples": [
                                        "[DATA_SOURCE]",
                                        "[ORIGIN_SYSTEM]",
                                        "[SOURCE_DESCRIPTION]"
                                    ]
                                }
                            }
                        }
                    },
                    "indexes": {
                        "type": "array",
                        "description": "Entity indexes using placeholders",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "examples": [
                                        "[INDEX_NAME]",
                                        "[INDEX_IDENTIFIER]"
                                    ]
                                },
                                "type": {
                                    "type": "string",
                                    "examples": [
                                        "[INDEX_TYPE]",
                                        "Primary",
                                        "Unique",
                                        "Composite",
                                        "Partial",
                                        "[CUSTOM_INDEX]"
                                    ]
                                },
                                "attributes": {
                                    "type": "array",
                                    "items": {
                                        "type": "string",
                                        "examples": [
                                            "[INDEXED_ATTRIBUTE]",
                                            "[INDEXED_FIELD]"
                                        ]
                                    }
                                },
                                "purpose": {
                                    "type": "string",
                                    "examples": [
                                        "[INDEX_PURPOSE]",
                                        "[PERFORMANCE_REASON]"
                                    ]
                                }
                            }
                        }
                    },
                    "partitioning": {
                        "type": "object",
                        "description": "Partitioning strategy using placeholders",
                        "properties": {
                            "strategy": {
                                "type": "string",
                                "examples": [
                                    "[PARTITIONING_STRATEGY]",
                                    "Range",
                                    "Hash",
                                    "List",
                                    "Composite",
                                    "[CUSTOM_PARTITIONING]"
                                ]
                            },
                            "partitionKey": {
                                "type": "string",
                                "examples": [
                                    "[PARTITION_KEY]",
                                    "[PARTITIONING_ATTRIBUTE]"
                                ]
                            },
                            "rationale": {
                                "type": "string",
                                "examples": [
                                    "[PARTITIONING_RATIONALE]",
                                    "[PERFORMANCE_JUSTIFICATION]"
                                ]
                            }
                        }
                    },
                    "archivalPolicy": {
                        "type": "object",
                        "description": "Data archival policy using placeholders",
                        "properties": {
                            "retentionPeriod": {
                                "type": "string",
                                "examples": [
                                    "[RETENTION_PERIOD]",
                                    "[DATA_LIFECYCLE]"
                                ]
                            },
                            "archivalStrategy": {
                                "type": "string",
                                "examples": [
                                    "[ARCHIVAL_STRATEGY]",
                                    "[ARCHIVAL_METHOD]"
                                ]
                            },
                            "complianceRequirement": {
                                "type": "string",
                                "examples": [
                                    "[COMPLIANCE_REQUIREMENT]",
                                    "[LEGAL_REQUIREMENT]"
                                ]
                            }
                        }
                    }
                }
            }
        },
        "relationships": {
            "type": "array",
            "description": "Entity relationships using placeholders",
            "items": {
                "type": "object",
                "required": [
                    "name",
                    "type",
                    "sourceEntity",
                    "targetEntity"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Relationship name - use placeholder [RELATIONSHIP_NAME]",
                        "examples": [
                            "[RELATIONSHIP_NAME]",
                            "[ASSOCIATION_NAME]",
                            "[REFERENCE_NAME]"
                        ]
                    },
                    "type": {
                        "type": "string",
                        "description": "Relationship type - use placeholder [RELATIONSHIP_TYPE]",
                        "examples": [
                            "[RELATIONSHIP_TYPE]",
                            "One-to-One",
                            "One-to-Many",
                            "Many-to-Many",
                            "Inheritance",
                            "Composition",
                            "Aggregation",
                            "[CUSTOM_RELATIONSHIP]"
                        ]
                    },
                    "sourceEntity": {
                        "type": "string",
                        "description": "Source entity - use placeholder [SOURCE_ENTITY]",
                        "examples": [
                            "[SOURCE_ENTITY]",
                            "[PARENT_ENTITY]",
                            "[FROM_ENTITY]"
                        ]
                    },
                    "targetEntity": {
                        "type": "string",
                        "description": "Target entity - use placeholder [TARGET_ENTITY]",
                        "examples": [
                            "[TARGET_ENTITY]",
                            "[CHILD_ENTITY]",
                            "[TO_ENTITY]"
                        ]
                    },
                    "description": {
                        "type": "string",
                        "description": "Relationship description - use placeholder [RELATIONSHIP_DESCRIPTION]",
                        "examples": [
                            "[RELATIONSHIP_DESCRIPTION]",
                            "[ASSOCIATION_PURPOSE]"
                        ]
                    },
                    "cardinality": {
                        "type": "object",
                        "description": "Relationship cardinality using placeholders",
                        "properties": {
                            "source": {
                                "type": "string",
                                "examples": [
                                    "[SOURCE_CARDINALITY]",
                                    "1",
                                    "0..1",
                                    "1..*",
                                    "0..*",
                                    "[CUSTOM_CARDINALITY]"
                                ]
                            },
                            "target": {
                                "type": "string",
                                "examples": [
                                    "[TARGET_CARDINALITY]",
                                    "1",
                                    "0..1",
                                    "1..*",
                                    "0..*",
                                    "[CUSTOM_CARDINALITY]"
                                ]
                            }
                        }
                    },
                    "attributes": {
                        "type": "array",
                        "description": "Relationship attributes using placeholders",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "examples": [
                                        "[RELATIONSHIP_ATTRIBUTE]",
                                        "[ASSOCIATION_PROPERTY]"
                                    ]
                                },
                                "dataType": {
                                    "type": "string",
                                    "examples": [
                                        "[ATTRIBUTE_DATA_TYPE]",
                                        "[PROPERTY_TYPE]"
                                    ]
                                },
                                "description": {
                                    "type": "string",
                                    "examples": [
                                        "[ATTRIBUTE_DESCRIPTION]",
                                        "[PROPERTY_PURPOSE]"
                                    ]
                                }
                            }
                        }
                    },
                    "constraints": {
                        "type": "array",
                        "description": "Relationship constraints using placeholders",
                        "items": {
                            "type": "string",
                            "examples": [
                                "[RELATIONSHIP_CONSTRAINT]",
                                "[REFERENTIAL_INTEGRITY_RULE]"
                            ]
                        }
                    },
                    "cascadeRules": {
                        "type": "object",
                        "description": "Cascade rules using placeholders",
                        "properties": {
                            "onUpdate": {
                                "type": "string",
                                "examples": [
                                    "[UPDATE_CASCADE_RULE]",
                                    "CASCADE",
                                    "RESTRICT",
                                    "SET NULL",
                                    "[CUSTOM_RULE]"
                                ]
                            },
                            "onDelete": {
                                "type": "string",
                                "examples": [
                                    "[DELETE_CASCADE_RULE]",
                                    "CASCADE",
                                    "RESTRICT",
                                    "SET NULL",
                                    "[CUSTOM_RULE]"
                                ]
                            }
                        }
                    }
                }
            }
        },
        "constraints": {
            "type": "object",
            "description": "Global constraints and business rules using placeholders",
            "properties": {
                "businessRules": {
                    "type": "array",
                    "description": "Business rules using placeholders",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "examples": [
                                    "[BUSINESS_RULE_NAME]",
                                    "[RULE_IDENTIFIER]"
                                ]
                            },
                            "description": {
                                "type": "string",
                                "examples": [
                                    "[BUSINESS_RULE_DESCRIPTION]",
                                    "[RULE_EXPLANATION]"
                                ]
                            },
                            "type": {
                                "type": "string",
                                "examples": [
                                    "[RULE_TYPE]",
                                    "Validation",
                                    "Calculation",
                                    "Derivation",
                                    "Constraint",
                                    "[CUSTOM_RULE_TYPE]"
                                ]
                            },
                            "scope": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[AFFECTED_ENTITY]",
                                        "[RULE_SCOPE]"
                                    ]
                                }
                            },
                            "implementation": {
                                "type": "string",
                                "examples": [
                                    "[RULE_IMPLEMENTATION]",
                                    "Database Trigger",
                                    "Application Logic",
                                    "Stored Procedure",
                                    "[IMPLEMENTATION_METHOD]"
                                ]
                            },
                            "priority": {
                                "type": "string",
                                "examples": [
                                    "[RULE_PRIORITY]",
                                    "High",
                                    "Medium",
                                    "Low",
                                    "[PRIORITY_LEVEL]"
                                ]
                            }
                        }
                    }
                },
                "dataQualityRules": {
                    "type": "array",
                    "description": "Data quality rules using placeholders",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "examples": [
                                    "[QUALITY_RULE_NAME]",
                                    "[DQ_RULE_ID]"
                                ]
                            },
                            "description": {
                                "type": "string",
                                "examples": [
                                    "[QUALITY_RULE_DESCRIPTION]",
                                    "[DQ_RULE_PURPOSE]"
                                ]
                            },
                            "dimension": {
                                "type": "string",
                                "examples": [
                                    "[QUALITY_DIMENSION]",
                                    "Completeness",
                                    "Accuracy",
                                    "Consistency",
                                    "Validity",
                                    "Uniqueness",
                                    "[CUSTOM_DIMENSION]"
                                ]
                            },
                            "entities": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[AFFECTED_ENTITY]",
                                        "[MONITORED_TABLE]"
                                    ]
                                }
                            },
                            "threshold": {
                                "type": "string",
                                "examples": [
                                    "[QUALITY_THRESHOLD]",
                                    "[ACCEPTABLE_LEVEL]"
                                ]
                            }
                        }
                    }
                },
                "securityConstraints": {
                    "type": "array",
                    "description": "Security constraints using placeholders",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "examples": [
                                    "[SECURITY_CONSTRAINT_NAME]",
                                    "[SECURITY_RULE_ID]"
                                ]
                            },
                            "type": {
                                "type": "string",
                                "examples": [
                                    "[SECURITY_TYPE]",
                                    "Access Control",
                                    "Encryption",
                                    "Masking",
                                    "Auditing",
                                    "[SECURITY_MEASURE]"
                                ]
                            },
                            "scope": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[PROTECTED_ENTITY]",
                                        "[SECURED_ATTRIBUTE]"
                                    ]
                                }
                            },
                            "implementation": {
                                "type": "string",
                                "examples": [
                                    "[SECURITY_IMPLEMENTATION]",
                                    "[PROTECTION_METHOD]"
                                ]
                            },
                            "complianceRequirement": {
                                "type": "string",
                                "examples": [
                                    "[COMPLIANCE_DRIVER]",
                                    "[REGULATORY_REQUIREMENT]"
                                ]
                            }
                        }
                    }
                }
            }
        },
        "dataGovernance": {
            "type": "object",
            "description": "Data governance policies using placeholders",
            "properties": {
                "dataOwnership": {
                    "type": "array",
                    "description": "Data ownership assignments using placeholders",
                    "items": {
                        "type": "object",
                        "properties": {
                            "entity": {
                                "type": "string",
                                "examples": [
                                    "[OWNED_ENTITY]",
                                    "[DATA_ASSET]"
                                ]
                            },
                            "owner": {
                                "type": "string",
                                "examples": [
                                    "[DATA_OWNER]",
                                    "[RESPONSIBLE_PARTY]"
                                ]
                            },
                            "steward": {
                                "type": "string",
                                "examples": [
                                    "[DATA_STEWARD]",
                                    "[CUSTODIAN]"
                                ]
                            },
                            "responsibilities": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[RESPONSIBILITY]",
                                        "[OWNERSHIP_DUTY]"
                                    ]
                                }
                            }
                        }
                    }
                },
                "dataLineage": {
                    "type": "array",
                    "description": "Data lineage information using placeholders",
                    "items": {
                        "type": "object",
                        "properties": {
                            "entity": {
                                "type": "string",
                                "examples": [
                                    "[LINEAGE_ENTITY]",
                                    "[DATA_ELEMENT]"
                                ]
                            },
                            "sources": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[DATA_SOURCE]",
                                        "[UPSTREAM_SYSTEM]"
                                    ]
                                }
                            },
                            "transformations": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[TRANSFORMATION_RULE]",
                                        "[DATA_PROCESSING_STEP]"
                                    ]
                                }
                            },
                            "consumers": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "examples": [
                                        "[DATA_CONSUMER]",
                                        "[DOWNSTREAM_SYSTEM]"
                                    ]
                                }
                            }
                        }
                    }
                },
                "privacyControls": {
                    "type": "array",
                    "description": "Privacy controls using placeholders",
                    "items": {
                        "type": "object",
                        "properties": {
                            "entity": {
                                "type": "string",
                                "examples": [
                                    "[PRIVACY_ENTITY]",
                                    "[PROTECTED_DATA]"
                                ]
                            },
                            "personalDataType": {
                                "type": "string",
                                "examples": [
                                    "[PERSONAL_DATA_TYPE]",
                                    "PII",
                                    "Sensitive Personal Data",
                                    "Biometric",
                                    "[DATA_CATEGORY]"
                                ]
                            },
                            "legalBasis": {
                                "type": "string",
                                "examples": [
                                    "[LEGAL_BASIS]",
                                    "Consent",
                                    "Contract",
                                    "Legal Obligation",
                                    "[PROCESSING_GROUND]"
                                ]
                            },
                            "retentionPeriod": {
                                "type": "string",
                                "examples": [
                                    "[RETENTION_PERIOD]",
                                    "[DATA_LIFECYCLE]"
                                ]
                            },
                            "anonymizationMethod": {
                                "type": "string",
                                "examples": [
                                    "[ANONYMIZATION_METHOD]",
                                    "[PRIVACY_TECHNIQUE]"
                                ]
                            }
                        }
                    }
                }
            }
        },
        "performanceConsiderations": {
            "type": "object",
            "description": "Performance considerations using placeholders",
            "properties": {
                "scalabilityRequirements": {
                    "type": "object",
                    "properties": {
                        "expectedVolume": {
                            "type": "string",
                            "examples": [
                                "[EXPECTED_VOLUME]",
                                "[DATA_GROWTH_PROJECTION]"
                            ]
